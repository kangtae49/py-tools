{
  "name": "py-tools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build --outDir gui --emptyOutDir --minify && uv run pyinstaller build-windows.spec",
    "lint": "eslint .",
    "preview": "vite preview",
    "models": "uv run pydantic2ts --module apps/models.py --output src/types/models.d.ts",
    "test": "uv run python -m unittest discover"
  },
  "dependencies": {
    "@blueprintjs/core": "^6.3.2",
    "@blueprintjs/icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@rexxars/react-split-pane": "^1.0.0",
    "@szhsin/react-menu": "^4.5.0",
    "natsort": "^2.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-mosaic-component": "^6.1.1",
    "react-router-dom": "^7.9.4",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.10",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
